N/*! Implementation of the active object start operation. */
Nvoid QActive_start_(QActive * const me, uint_fast8_t prio,
N                    QEvt const *qSto[], uint_fast16_t qLen,
N                    void *stkSto, uint_fast16_t stkSize,
