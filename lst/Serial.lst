N/* Implementation of putchar (also used by printf function to output data)    */
Nint send_char (int ch)  {                 /* Write character to Serial Port    */
N
N  while (!(UxLSR & 0x20));
X  while (!((*(volatile unsigned long *)(0xE000C000 + 0x14)) & 0x20));
N
N  return (UxTHR = ch);
X  return ((*(volatile unsigned long *)(0xE000C000 + 0x00)) = ch);
N}
