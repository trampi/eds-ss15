N/* Use serial 1  for polling  - serial 0 for interrupt*/
Nvoid init_serial (void)  {               /* Initialize Serial Interface       */
N  #ifdef UART0
N    PINSEL0 |= 0x00000050;               /* Enable TxD0 and RxD0              */
X    (*(volatile unsigned long *)(0xE002C000 + 0x00)) |= 0x00000050;                
N  #elif defined (UART1)
S    PINSEL0 |= 0x40000000;               /* Enable TxD1                       */
S    PINSEL1 |= 0x00000001;               /* Enable RxD1                       */
N  #endif
N  UxFDR    = 0;                          /* Fractional divider not used       */
X  (*(volatile unsigned long *)(0xE000C000 + 0x28))    = 0;                           
N  UxLCR    = 0x83;                       /* 8 bits, no Parity, 1 Stop bit     */
X  (*(volatile unsigned long *)(0xE000C000 + 0x0C))    = 0x83;                        
N  UxDLL    = 78;                         /* 9600 Baud Rate @ 12.0 MHZ PCLK    */
X  (*(volatile unsigned long *)(0xE000C000 + 0x00))    = 78;                          
N  UxDLM    = 0;                          /* High divisor latch = 0            */
X  (*(volatile unsigned long *)(0xE000C000 + 0x04))    = 0;                           
N  UxLCR    = 0x03;                       /* DLAB = 0                          */
X  (*(volatile unsigned long *)(0xE000C000 + 0x0C))    = 0x03;                        
N}
